<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!-- Testing 3d Models -->
<launch>
  <param name="use_sim_time" value="true" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find object_recognition_renderer)/resources/mesh.rviz" />
  <node name="playback" pkg="rosbag" type="play" args="$(find object_recognition_renderer)/resources/test.bag --clock -r 0.1" />
</launch>
